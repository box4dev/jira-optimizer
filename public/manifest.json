{
  "manifest_version": 3,
  "version": "1.1.3",
  "default_locale": "en_US",
  "description": "__MSG_extDesc__",
  "name": "__MSG_extName__",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": [
        "*://*.atlassian.net/*",
        "*://*/*"
      ],
      "js": ["contentScript.js"],
      "css": ["styles.css"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
